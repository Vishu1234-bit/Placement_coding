def areEquallyStrong(yourLeft, yourRight, friendsLeft, friendsRight):
    your = yourLeft+yourRight
    friend = friendsLeft+friendsRight
    maxyour = max(yourLeft,yourRight)
    maxfriend = max(friendsLeft,friendsRight)
    if((your==friend) & (maxyour == maxfriend)):
        return True
    else:
        return False
yourLeft = 10
yourRight = 15
friendsLeft = 15
friendsRight = 10 
print(areEquallyStrong(yourLeft, yourRight, friendsLeft, friendsRight) )
